{
	"name": "webutils",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"build": "pnpm test && svelte-kit build",
		"build:static": "cross-env ADAPTER=static npm run build",
		"build:node": "cross-env ADAPTER=node npm run build",
		"build:netlify": "cross-env ADAPTER=netlify npm run build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@smui/radio": "^6.0.0-beta.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.45",
		"@sveltejs/adapter-node": "^1.0.0-next.68",
		"@sveltejs/adapter-static": "^1.0.0-next.28",
		"@sveltejs/kit": "next",
		"@types/lodash": "^4.14.178",
		"@types/node": "*",
		"@typescript-eslint/eslint-plugin": "^5.11.0",
		"@typescript-eslint/parser": "^5.11.0",
		"c8": "^7.11.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"node-sass": "^7.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"sass": "^1.49.7",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.3",
		"svelte-grid": "^5.1.1",
		"svelte-preprocess": "^4.10.3",
		"ts-node": "^10.5.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5",
		"vitest": "^0.3.4"
	},
	"type": "module",
	"dependencies": {
		"lodash": "^4.17.21",
		"nerdamer": "^1.1.13",
		"svelte-click-outside": "^1.0.0",
		"vite-tsconfig-paths": "^3.4.0"
	}
}
